{
  "name": "search-star-wars-3",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "format": "bash format.sh",
    "deploy": "npm run build && npx gh-pages -d dist",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:css": "stylelint \"**/*.{vue,scss,css}\" --fix",
    "lint:vue": "eslint \"src/**/*.vue\" --fix",
    "lint:vue:check": "eslint \"src/**/*.vue\"",
    "lint:attributes": "eslint \"src/**/*.vue\" --fix --rule 'vue/attributes-order: error'",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.6",
    "@mdi/font": "7.4.47",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.0.13",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.86.3",
    "stylelint": "^16.21.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.8.3",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^7.0.0",
    "vite-plugin-checker": "^0.10.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.1"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
